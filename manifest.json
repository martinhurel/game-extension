{
    "manifest_version": 2,
    "name": "Game Helper",
    "description": "Game helper",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["Content.js"]
        }
    ],
    "icons": {"128": "icon_128.png" },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}